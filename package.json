{
  "name": "fapp",
  "version": "1.0.0",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "yarn build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\" -R spec",
    "test:only": "tap --ts \"test/**/*.test.ts\"",
    "start": "yarn build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:test:ts": "yarn watch:ts && tsc -w -p test/tsconfig.json",
    "watch:ts": "tsc -w",
    "dev": "yarn build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"yarn:watch:ts\" \"yarn:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/env": "^4.2.0",
    "@fastify/sensible": "^5.0.0",
    "@sinclair/typebox": "^0.25.13",
    "debug": "^4.3.4",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.0",
    "fastify-plugin": "^4.0.0",
    "mongoose": "^6.8.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tape": "^5.6.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
